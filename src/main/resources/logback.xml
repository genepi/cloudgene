<configuration>


	<appender name="FILE" class="ch.qos.logback.core.FileAppender">
	    <file>cloudgene.log</file>
	    <append>true</append>
	    <encoder>
	        <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{35} - %msg%n</pattern>
	    </encoder>
	</appender>
   

    <root level="info">
        <appender-ref ref="FILE" />
    </root>
</configuration>
